@model IEnumerable<GEOnet.Models.geoUser>
@{
    ViewData["Title"] = "Index";
}


<!DOCTYPE html>

<html>
<head>
    <body>
        @*<h3>Пользователи</h3>*@
        <form asp-controller="Home" asp-action="Index" method="get">
            <p>
                Пользователь: <input type="text" name="SearchUser" />
                Дата: <input type="text" name="SearchDate" />
                <input type="submit" value="Фильтровать" />
            </p>
        </form>
        <table align="center" border="1">
            <tr align="center">
                <th align="center" width="20%">Пользователь</th>
                <th align="center" width="20%">Устройство</th>
                <th align="center" width="15%">Время</th>
                <th align="center" width="15%">Дата</th>
                <th align="center" width="15%">Координаты</th>
                <th align="center" width="15%">Карта</th>

            </tr>
            @foreach (var geomod in Model)
            {
        <tr align="center">
            <td align="center" >@geomod.username</td>
            <td align="center" >@geomod.namedevice</td>
            <td align="center" >@geomod.tm</td>
            <td align="center" >@geomod.dt</td>
            @*нужно убрать <td align="center" width="20%"><a href="~/Home/printname?searchName=@geomod.username">Смотреть</a></td>*@
            <td align="center" ><a href="~/Home/searchnamedev?searchName=@geomod.username&searchDev=@geomod.namedevice">Отобразить координаты</a></td>
            <td align="center" ><a href="~/Home/geomapall?Name=@geomod.username&Dev=@geomod.namedevice">Отобразить на карте</a></td>
        </tr>
            }
        </table>
    </body>
</head>
</html>